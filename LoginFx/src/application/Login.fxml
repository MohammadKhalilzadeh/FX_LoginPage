      <TextField fx:id="txtUserName" alignment="CENTER" layoutX="39.0" layoutY="37.0" promptText="UserName" style="-fx-background-color: #faebd7;">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txPassword" alignment="CENTER" layoutX="39.0" layoutY="111.0" promptText="Password" style="-fx-background-color: #faebd7;">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" alignment="CENTER" layoutX="39.0" layoutY="170.0" prefHeight="39.0" prefWidth="224.0" style="-fx-background-color: #faebd7;" text="Status" textFill="#fc0000">
